{
    "manifest_version":2,
    "name":"Dictionary",
    "version":"0.1",
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js":["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_title": "Dictionary",
        "default_popup": "index.html"
    }

  
}